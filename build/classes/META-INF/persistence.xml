<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="formm"  transaction-type="JTA">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<jta-data-source>jdbc/inscription</jta-data-source>
		<class>model.Cookie</class>
		<class>model.Form</class>
		<class>model.Input</class>
		<class>model.ListValue</class>
		<class>model.ResetPassword</class>
		<class>model.SubmittedForm</class>
		<class>model.Type</class>
		<class>model.User</class>
		<class>model.Value</class>
      
	</persistence-unit>
</persistence>
